{
   "name": "Anki Vocabulary",
   "permissions": [
      "storage",
      "notifications",
      "contextMenus"
   ],
   "manifest_version": 3,
   "action": {
      "default_icon": {
         "128": "images/dictionary-icon.png",
         "16": "images/dictionary-icon.png",
         "32": "images/dictionary-icon.png",
         "48": "images/dictionary-icon.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "js/background.js"
   },
   "description": "Easily save words and get instant translations. Review, export for Anki, and accelerate language learning.",
   "icons": {
      "128": "images/dictionary-icon.png",
      "16": "images/dictionary-icon.png",
      "32": "images/dictionary-icon.png",
      "48": "images/dictionary-icon.png"
   },

   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.0"
}